#include <stdio.h>

int main(int argc, char* argv[])
{ 
  // Make sure we have the right number of arguments.
  if(argc != 3)
    {
      printf("Specify thread count and time(seconds)");
      return 0;
    }

  int threadCount, runTime;
  threadCount = atoi(argv[1]);
  runTime = atoi(atgv[2]);
  if(threadCount < 1 || threadCount > 100)
    {
      printf("ThreadCount must be between 1 and 99");
      return 0;
    }

  if(runTime < 1)
    {
      printf("Runtime must be > 0");
      return 0;
    }
  printf("ThreadCount: %i, Runtime %i", threadCount, runTime);
}
